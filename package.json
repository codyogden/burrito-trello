{
  "name": "trello",
  "version": "0.0.1",
  "description": "A better Trello for node.",
  "main": "index.js",
  "author": "Cody Ogden <cody@codyogden.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"lib/**/*.{js,jsx}\"",
    "docs": "yarn jsdoc ./lib -r -d ./docs -R ./README.md && http-server -p 8044 ./docs/ -o -s -d false -P https://docs.burrito.work",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "watch": "rollup -c --watch",
    "build": "yarn docs && yarn run lint && yarn format && yarn run rollup -c"
  },
  "dependencies": {
    "axios": "^0.17.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.14.9",
    "prettier": "^1.10.2",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
